<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:datavis="clr-namespace:System.Windows.Controls.DataVisualization;assembly=System.Windows.Controls.DataVisualization.Toolkit"
    xmlns:toolKit="clr-namespace:System.Windows.Controls.DataVisualization.Charting;assembly=System.Windows.Controls.DataVisualization.Toolkit"
    xmlns:primitives="clr-namespace:System.Windows.Controls.DataVisualization.Charting.Primitives;assembly=System.Windows.Controls.DataVisualization.Toolkit"
    xmlns:local="clr-namespace:HuaWeiCtls"
    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <Style TargetType="{x:Type local:ReflectedButton}">
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type local:ReflectedButton}">
                    <Grid ClipToBounds="True">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="3*"/>
                            <RowDefinition Height="3"/>
                            <RowDefinition Height="1*"/>
                        </Grid.RowDefinitions>
                        <Grid x:Name="PART_Container" Cursor="Hand">
                            <Grid.RowDefinitions>
                                <RowDefinition/>
                                <RowDefinition/>
                                <RowDefinition/>
                            </Grid.RowDefinitions>
                            <Border Grid.RowSpan="3" CornerRadius="4" ClipToBounds="True" SnapsToDevicePixels="True">
                                <Border.Background>
                                    <ImageBrush x:Name="BtnImageBrush" Stretch="Fill"/>
                                </Border.Background>
                            </Border>
                            <Border x:Name="PART_BottomBorder" Grid.Row="2" BorderBrush="Gray" CornerRadius="0,0,3,3" 
                                    Margin="0,1,0,0" SnapsToDevicePixels="True" Background="{TemplateBinding BottomBrush}"/>
                            <Rectangle Grid.Row="2" Height="1" VerticalAlignment="Top" Fill="#60000000"/>
                            <Rectangle Grid.Row="2" Margin="0,1,0,0" Height="1" VerticalAlignment="Top" Fill="#40FFFFFF"/>

                            <Border Grid.RowSpan="3" CornerRadius="3" ClipToBounds="True" SnapsToDevicePixels="True">
                                <Rectangle Fill="#30FFFFFF" RenderTransformOrigin="0.5,0.5" Margin="20,-45,-20,20">
                                    <Rectangle.RenderTransform>
                                        <TransformGroup>
                                            <RotateTransform Angle="-102"/>
                                            <TranslateTransform X="50" Y="25"/>
                                        </TransformGroup>
                                    </Rectangle.RenderTransform>
                                </Rectangle>
                            </Border>

                            <Border x:Name="MaskBorder" Grid.RowSpan="3" CornerRadius="3" Background="Transparent"/>

                            <TextBlock x:Name="PART_TextBlock" Grid.RowSpan="2" TextWrapping="Wrap"  Foreground="White" FontSize="16"
                                       Text="{TemplateBinding Text}" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                        </Grid>
                        <Rectangle x:Name="ReflectRectangle" Grid.Row="2" VerticalAlignment="Top" HorizontalAlignment="Stretch">
                            <Rectangle.Fill>
                                <!-- Creates the reflection. -->
                                <VisualBrush Opacity="0.5" Stretch="None" Visual="{Binding ElementName=PART_Container}">
                                    <VisualBrush.RelativeTransform>
                                        <!-- Flip the reflection. -->
                                        <TransformGroup>
                                            <ScaleTransform ScaleX="1" ScaleY="-1" />
                                            <TranslateTransform  Y="1" />
                                        </TransformGroup>
                                    </VisualBrush.RelativeTransform>
                                </VisualBrush>
                            </Rectangle.Fill>
                            <Rectangle.OpacityMask>
                                <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                                    <GradientStop Color="#FF000000" Offset="0.0" />
                                    <GradientStop Color="#40000000" Offset="0.22" />
                                    <GradientStop Color="#00000000" Offset="0.33" />
                                </LinearGradientBrush>
                            </Rectangle.OpacityMask>
                            <Rectangle.BitmapEffect>
                                <BlurBitmapEffect Radius="1.5" />
                            </Rectangle.BitmapEffect>
                        </Rectangle>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="NavigationButtonStyle" TargetType="{x:Type Button}">
        <Setter Property="FocusVisualStyle" Value="{x:Null}" />
        <Setter Property="Foreground" Value="#E0E0E0" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="HorizontalContentAlignment" Value="Center" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="Padding" Value="1" />
        <Setter Property="Width" Value="20" />
        <Setter Property="Height" Value="100" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Grid>
                        <Border x:Name="border" SnapsToDevicePixels="true" Background="#80000000" Opacity="0.60"/>
                        <ContentPresenter x:Name="contentPresenter" Focusable="False" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                          Margin="{TemplateBinding Padding}" RecognizesAccessKey="True" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
                                          VerticalAlignment="{TemplateBinding VerticalContentAlignment}" />
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="true">
                            <Setter Property="Background" TargetName="border" Value="#90000000" />
                            <Setter Property="TextElement.Foreground" Value="White" />
                        </Trigger>
                        <Trigger Property="IsPressed"  Value="true">
                            <Setter Property="Background" TargetName="border" Value="#A0000000" />
                            <Setter Property="TextElement.Foreground" Value="White" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="false">
                            <Setter Property="Visibility" Value="Collapsed" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="{x:Type local:FlipView}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type local:FlipView}">
                    <Border Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}">
                        <Grid ClipToBounds="True" x:Name="PART_Container">
                            <local:FlipViewPanel x:Name="PART_Root" Background="Transparent">
                                <ContentControl x:Name="PART_PreviousItem"
                                                ContentTemplate="{Binding ItemTemplate, RelativeSource={RelativeSource TemplatedParent}}" />
                                <ContentControl x:Name="PART_NextItem"
                                                ContentTemplate="{Binding ItemTemplate, RelativeSource={RelativeSource TemplatedParent}}" />
                                <ContentControl x:Name="PART_CurrentItem"
                                                ContentTemplate="{Binding ItemTemplate, RelativeSource={RelativeSource TemplatedParent}}" />
                            </local:FlipViewPanel>
                            <Grid VerticalAlignment="Center" x:Name="PART_ButtonPanel" Visibility="Collapsed">
                                <Button x:Name="PART_PreviousButton" Content="&lt;"
                                        FontSize="20" Style="{StaticResource NavigationButtonStyle}"
                                        Command="{x:Static local:FlipView.PreviousCommand}" HorizontalAlignment="Left" />
                                <Button x:Name="PART_NextButton" Content="&gt;"
                                        FontSize="20" Style="{StaticResource NavigationButtonStyle}"
                                        Command="{x:Static local:FlipView.NextCommand}" HorizontalAlignment="Right" />
                            </Grid>
                        </Grid>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter Property="Visibility" Value="Visible" TargetName="PART_ButtonPanel"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

